import java.util.*;
import java.lang.Math;
public class Main{

     public static void main(String []args){
        
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int temp=num;
        while (true)
        {
            double sqno=Math.sqrt(temp);
            double cbno=Math.cbrt(temp);
            if(sqno==(int)sqno && cbno==(int)cbno)
            {
                System.out.print(temp);
                break;
            }
            temp-=1;
        }
        
     }
}
